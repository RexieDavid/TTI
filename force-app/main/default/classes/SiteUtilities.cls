    @InvocableMethod(label='Update session attendee unique attendance code' description='Update session attendee unique attendance code')
    public static void updateSessionAttendeeAttendanceCode(List<Session_Attendee__c> attendees) {
        List<Session_Attendee__c> attendeeList = [SELECT Id,
                                                         Session__c, 
                                                         Session__r.Workshop__c,
                                                         Session__r.Workshop__r.Event__c,
                                                         Attendee__c,
                                                         Unique_Attendance_Code__c
                                                    FROM Session_Attendee__c 
                                                   WHERE Id = :(new Map<Id, Session_Attendee__c>(attendees).keySet())];
        for (Session_Attendee__c a : attendeeList) {
            a.Unique_Attendance_Code__c = SiteUtilities.encrypt(String.valueOf(a.Id) +
                                                                String.valueOf(a.Session__c) + 
                                                                String.valueOf(a.Attendee__c) + 
                                                                String.valueOf(a.Session__r.Workshop__c) + 
                                                                String.valueOf(a.Session__r.Workshop__r.Event__c));
            //a.Unique_Attendance_Code__c = a.Id;
        }
        update attendeeList;
    }
    
    public static String encrypt(String strToEncrypt) {
        Blob key = Crypto.generateAesKey(128);
        Blob data = Blob.valueOf(strToEncrypt);
        Blob encrypted = Crypto.encryptWithManagedIV('AES128', key, data);
        return EncodingUtil.base64Encode(encrypted);
    }

