    @AuraEnabled
    public static String getPickListValuesToJSON(String mapObjAndPicklistJSON) {
        Map<String, Map<String, List<Schema.PicklistEntry>>> mapObjAndPicklistValues = new Map<String, Map<String, List<Schema.PicklistEntry>>>();
        Map<String, Set<String>> mapObjAndPicklist = null;
        String jsonPicklistValues = null;

        if(String.isNotBlank(mapObjAndPicklistJSON))
            mapObjAndPicklist = (Map<String, Set<String>>) JSON.deserialize(mapObjAndPicklistJSON, Map<String, Set<String>>.class);
        else
            return null;

        if(!mapObjAndPicklist.isEmpty()) {

            for(String objName :mapObjAndPicklist.keySet()) {
                if(!mapObjAndPicklist.get(objName).isEmpty()) {

                    Map<String, List<Schema.PicklistEntry>> picklistValues = new Map<String, List<Schema.PicklistEntry>>();
                    for(String fieldName :mapObjAndPicklist.get(objName)) {
                        picklistValues.put(fieldName, UtilityClass.getPicklistValues(objName, fieldName));
                    }

                    mapObjAndPicklistValues.put(objName, picklistValues);
                }
            }

            if(!mapObjAndPicklistValues.isEmpty()) 
                jsonPicklistValues = JSON.serialize(mapObjAndPicklistValues);
        }

        return jsonPicklistValues;
    }
    
