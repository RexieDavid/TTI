<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute type="String" name="PageName" default="MILRedemptionPage" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:registerEvent name="navigate" type="c:NavigateEvent" />
    <aura:handler event="c:NavigateEvent" action="{!c.handleNavigate}" />
    <div aura:id="mainDiv">
        <div class="slds-grid slds-wrap slds-p-top_xx-large">
            <div class="slds-col slds-size_1-of-1 slds-grid slds-wrap">
                <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_2-of-3 slds-grid slds-grid_align-center slds-p-top_small">
                    <div class="slds-size_10-of-12 slds-grid slds-wrap">
                        <div class="slds-size_1-of-1">
                            <div style="background-color: #F27828;">
                                <div class="slds-align_absolute-center">	
                                    <div class="slds-text-title_caps slds-text-align_center">
                                        <div class="slds-text-heading_large slds-text-color_inverse">
                                            Register and Redeem
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="slds-size_1-of-1 slds-grid slds-wrap slds-grid_align-spread">
                            <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_2-of-5 slds-grid slds-wrap slds-p-top_small slds-grid_align-center">
                                <div class="slds-size_3-of-5 slds-large-size_1-of-1 slds-box slds-box_x-small slds-grid border-color_orange">
                                    <div class="slds-col slds-size_1-of-8 slds-text-align_center" >
                                        <span class="slds-icon_container" >
                                            <c:svg class="slds-icon slds-icon_small icon-text_orange" xlinkHref="{!$Resource.SLDS252 + '/icons/action-sprite/svg/symbols.svg#share_thanks'}" />
                                        </span>                                     
                                    </div>
                                    <div class="slds-col slds-size_6-of-8 slds-box slds-box_x-small slds-text-align_center border-color_orange">
                                        Register your purchased product and redeem bonus
                                    </div>
                                    <div class="slds-col slds-size_1-of-8 slds-text-align_center" >
                                        <span class="slds-icon_container" >
                                            <c:svg class="slds-icon slds-icon_small icon-text_orange" xlinkHref="{!$Resource.SLDS252 + '/icons/utility-sprite/svg/symbols.svg#chevronright'}" />
                                        </span>
                                    </div>
                                </div>   
                                <div class="slds-size_1-of-1 slds-align_absolute-center slds-hide_large">
                                    <lightning:button variant="brand" label="REGISTER NOW" onclick="{!c.navigatetotoolbag}" class="globalProductItemButton2" />
                                </div>                               
                            </div>

                            <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_2-of-5 slds-grid slds-wrap slds-p-top_small slds-grid_align-center">              
                                <div class="slds-size_3-of-5 slds-large-size_1-of-1 slds-box slds-box_x-small slds-grid border-color_orange">
                                    <div class="slds-col slds-size_1-of-8 slds-text-align_center" >
                                        <span class="slds-icon_container" >
                                            <c:svg class="slds-icon slds-icon_small icon-text_orange" xlinkHref="{!$Resource.SLDS252 + '/icons/action-sprite/svg/symbols.svg#share_thanks'}" />
                                        </span>                                     
                                    </div>
                                    <div class="slds-col slds-size_6-of-8 slds-box slds-box_x-small slds-text-align_center border-color_orange">
                                        View unclaimed redemptions for registered products
                                    </div>
                                    <div class="slds-col slds-size_1-of-8 slds-text-align_center" >
                                        <span class="slds-icon_container" >
                                            <c:svg class="slds-icon slds-icon_small icon-text_orange" xlinkHref="{!$Resource.SLDS252 + '/icons/utility-sprite/svg/symbols.svg#chevronright'}" />
                                        </span>
                                    </div>
                                </div>   
                                <div class="slds-size_1-of-1 slds-align_absolute-center slds-hide_large">
                                    <lightning:button variant="brand" label="CLAIM NOW" onclick="{!c.navigateToActivePromotions}" class="globalProductItemButton2" />
                                </div> 
                            </div>
                        </div>
                    </div>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_1-of-3 slds-grid slds-grid_align-center slds-p-top_small">
                    <div class="slds-size_11-of-12 slds-small-size_10-of-12 slds-grid slds-wrap">
                        <div class="slds-size_1-of-1">
                            <div style="background-color: #F27828;">
                                <div class="slds-align_absolute-center">
                                    <div class="slds-text-title_caps slds-text-align_center">
                                        <div class="slds-text-heading_large slds-text-color_inverse">
                                            My Redemptions
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>     
                        <div class="slds-size_1-of-1 slds-grid slds-grid_align-center slds-p-top_small slds-wrap">
                            <div class="slds-size_3-of-5 slds-large-size_8-of-12 slds-box slds-box_x-small slds-grid border-color_orange">
                                <div class="slds-col slds-size_1-of-8 slds-text-align_center" >
                                    <span class="slds-icon_container" >
                                        <c:svg class="slds-icon slds-icon_small icon-text_orange" xlinkHref="{!$Resource.SLDS252 + '/icons/action-sprite/svg/symbols.svg#share_thanks'}" />
                                    </span>                                     
                                </div>
                                <div class="slds-col slds-size_6-of-8 slds-box slds-box_x-small slds-text-align_center border-color_orange">
                                    View and track redemptions
                                </div>
                                <div class="slds-col slds-size_1-of-8 slds-text-align_center" >
                                    <span class="slds-icon_container" >
                                        <c:svg class="slds-icon slds-icon_small icon-text_orange" xlinkHref="{!$Resource.SLDS252 + '/icons/utility-sprite/svg/symbols.svg#chevronright'}" />
                                    </span>
                                </div>
                            </div>   
                            <div class="slds-size_1-of-1 slds-align_absolute-center slds-hide_large">
                                <lightning:button variant="brand" label="VIEW NOW" onclick="{!c.navigateToRedemptionList}" class="globalProductItemButton2" />
                            </div> 
                        </div>                     
                    </div>
                </div>
            </div>
            <div class="slds-col slds-size_1-of-1 slds-grid slds-wrap slds-show_large">
                <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_2-of-3 slds-grid slds-grid_align-center slds-p-top_small">
                    <div class="slds-size_10-of-12 slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-grid slds-wrap slds-grid_align-spread">
                            <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_2-of-5 slds-grid slds-wrap slds-p-top_small slds-grid_align-center">
                                <div class="slds-size_1-of-1 slds-align_absolute-center">
                                    <lightning:button variant="brand" label="REGISTER NOW" onclick="{!c.navigatetotoolbag}" class="globalProductItemButton2" />
                                </div>                               
                            </div>

                            <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_2-of-5 slds-grid slds-wrap slds-p-top_small slds-grid_align-center">  
                                <div class="slds-size_1-of-1 slds-align_absolute-center">
                                    <lightning:button variant="brand" label="CLAIM NOW" onclick="{!c.navigateToActivePromotions}" class="globalProductItemButton2" />
                                </div> 
                            </div>
                        </div>
                    </div>
                </div>
                <div class="slds-col slds-size_1-of-1 slds-small-size_1-of-1 slds-large-size_1-of-3 slds-grid slds-grid_align-center slds-p-top_small">
                    <div class="slds-size_11-of-12 slds-small-size_10-of-12 slds-grid slds-wrap">   
                        <div class="slds-size_1-of-1 slds-grid slds-grid_align-center slds-p-top_small slds-wrap"> 
                            <div class="slds-size_1-of-1 slds-align_absolute-center">
                                <lightning:button variant="brand" label="VIEW NOW" onclick="{!c.navigateToRedemptionList}" class="globalProductItemButton2" />
                            </div> 
                        </div>                     
                    </div>
                </div>
            </div>
            <div class="slds-col slds-size_12-of-12 slds-p-vertical_x-large">
                <div class="slds-align_absolute-center">
                    <img src="{!$Resource.AEG_BackToWork}" />
                </div>
            </div>
        </div>
    </div>
</aura:component>