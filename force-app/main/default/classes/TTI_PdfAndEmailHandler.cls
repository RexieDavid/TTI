public class TTI_PdfAndEmailHandler {
    public static EmailTemplate getEmailTemplate(string EmailTemplateName){
        TTI_Template__c objTemplateName = TTI_Template__c.getValues(EmailTemplateName);
        List<EmailTemplate> lstEmailTemplate = [SELECT Id, Subject, HtmlValue FROM EmailTemplate WHERE Name =: objTemplateName.Approved_Claim_Notification_Template__c];
        return lstEmailTemplate[0];
    }

    /* Francis Nasalita 
    *  [SP-865] - 2018/11/13
    *  Added List<String> listToAddress to the parameter and set setToAddresses for Email
    */
    public static Messaging.SingleEmailMessage getEmailSendObj(string emailBody,string emailSubject,string targetObjectId,string whatId,string orgWideEmailId,Messaging.EmailFileAttachment objAttachToSend, List<String> listToAddress)
    {
        Messaging.SingleEmailMessage emailTobeSent = new Messaging.SingleEmailMessage();
        emailTobeSent.setHtmlBody(emailBody);
        emailTobeSent.setTargetObjectId(targetObjectId);
        emailTobeSent.setWhatId(whatId);
        emailTobeSent.setSaveAsActivity(true);
        emailTobeSent.setSubject(emailSubject);
        if(objAttachToSend!=null){
            emailTobeSent.setFileAttachments(new Messaging.EmailFileAttachment[] { objAttachToSend });
        }
        emailTobeSent.setOrgWideEmailAddressId(orgWideEmailId);
        emailTobeSent.setToAddresses(listToAddress);
        return emailTobeSent;
    }
    public static Messaging.EmailFileAttachment getAttachmentObjToSend(string fileName,Blob body){
        Messaging.EmailFileAttachment attach = new Messaging.EmailFileAttachment();
        attach.setContentType('application/pdf');
        attach.setFileName(fileName);
        attach.setInline(false);
        attach.Body = body;
        return attach;
    }
    public static void insertAttachmentToCase(string AttachmentName,Blob body,string parentId){
        Attachment ObjAttach = new Attachment();
        ObjAttach.Name = AttachmentName;
        ObjAttach.Body = body;
        ObjAttach.IsPrivate = false;
        ObjAttach.parentid = parentId;
        ObjAttach.contentType = 'application/pdf';
        insert ObjAttach;
    }
    
    public static void handleEmailAndAttachmentProcess(Case objCase,string operation){
        // Francis Nasalita [SP-865] 2018/11/13 - Added SuppliedEmail to query
        Case caseObj=[SELECT Id,CaseNumber,Brand__c,Product_Name__r.Name,TTI_Brand_Logo_URL__c,TTI_Customer_Contact__c,TTI_Quote_to_Retailer__c,
                      Retailer_Account__c,Retailer_Email_Address__c,Asset.Name, TTI_Customer_Contact__r.FirstName,
                      TTI_CaseFromEmailId__c,TTI_Email_Notification_Opt_In__c,TTI_Brand_Service_Phone__c,SuppliedEmail FROM Case WHERE Id=:objCase.Id];
        if(operation=='ReceiptGeneration')
        {
            EmailTemplate objEmailTemplate = TTI_PdfAndEmailHandler.getEmailTemplate('CaseReceiptTemplate');
            string emailBody=objEmailTemplate.HtmlValue;
            string emailSubject=objEmailTemplate.Subject;
            emailSubject = emailSubject.replace('CASE_CASENUMBER', caseObj.CaseNumber);
            emailSubject = emailSubject.replace('CASE_BRAND', caseObj.Brand__c);
            PageReference pdf = Page.TTI_CaseReceipt;
            string caseId=string.valueOf(caseObj.Id);
            pdf.getParameters().put('caseId',caseId);
            pdf.setRedirect(false);
            //System.debug('body >> ' + pdf.getContentAsPDF());
            Blob body;
            if(!Test.isRunningTest()){
               // body = pdf.getContent();
                body = pdf.getContentAsPDF();
                //System.debug('body >> ' + pdf.getContentAsPDF());
            }else{
                Test.setCurrentPage(pdf);
                TTI_CaseReceiptController ObjTTI_CaseReceiptController=new TTI_CaseReceiptController();
                body=Blob.valueof('QWERTRERTYTRT');
            }

            if(caseObj.TTI_Customer_Contact__r.FirstName != null && caseObj.TTI_Customer_Contact__r.FirstName != ''){
                emailBody = emailBody.replace('CASE_CONTACT_FIRST_NAME', caseObj.TTI_Customer_Contact__r.FirstName);
            }else{
                emailBody = emailBody.replace('CASE_CONTACT_FIRST_NAME', '');
            }
            
            emailBody = emailBody.replace('CASE_BRAND', caseObj.Brand__c);
            emailBody = emailBody.replace('CASE_PRODUCT_NAME_NAME', caseObj.Asset.Name);
            emailBody = emailBody.replace('CASE_CASENUMBER', caseObj.CaseNumber);
            emailBody = emailBody.replace('Brand_LogoURL', caseObj.TTI_Brand_Logo_URL__c);
            List<Messaging.SingleEmailMessage> lstemailToSent = new List<Messaging.SingleEmailMessage>();
            Messaging.EmailFileAttachment objAttachToSend = TTI_PdfAndEmailHandler.getAttachmentObjToSend(caseObj.CaseNumber+'_Receipt.pdf',body);
            // Francis Nasalita [SP-865] 2018/11/13 - Passing list of Email Address getEmailSendObj
            List<String> listToAddress = new List<String>();
            listToAddress.add(caseObj.SuppliedEmail);
            lstemailToSent.add(TTI_PdfAndEmailHandler.getEmailSendObj(emailBody,emailSubject,caseObj.TTI_Customer_Contact__c,caseObj.Id,caseObj.TTI_CaseFromEmailId__c,objAttachToSend,listToAddress));
            if(lstemailToSent.size() > 0 && caseObj.TTI_Email_Notification_Opt_In__c){
                Messaging.SendEmailResult [] r1 = Messaging.sendEmail(lstemailToSent);  
            }else{
                TTI_PdfAndEmailHandler.insertAttachmentToCase(caseObj.CaseNumber+'_Receipt.pdf',body,caseObj.id);
            }
        }
        if(operation=='QuoteGeneration')
        {
            EmailTemplate objEmailTemplate = TTI_PdfAndEmailHandler.getEmailTemplate('QuoteTemplate');
            string emailBody=objEmailTemplate.HtmlValue;
            string emailSubject=objEmailTemplate.Subject;
            emailSubject = emailSubject.replace('CASE_CASENUMBER', caseObj.CaseNumber);
            PageReference pdf = Page.TTI_Quotepdf;
            string caseId=string.valueOf(caseObj.Id);
            pdf.getParameters().put('caseId',caseId);
            pdf.setRedirect(false);
            Blob body;
            if(!Test.isRunningTest()){
                body = pdf.getContent();
            }else{
                Test.setCurrentPage(pdf);
                TTI_Quotepdf_controller ObjTTI_Quotepdf_controller=new TTI_Quotepdf_controller();
                body=Blob.valueof('QWERTRERTYTRT');
            }
            emailBody = emailBody.replace('CASE_BRAND', caseObj.Brand__c);
            emailBody = emailBody.replace('CASE_PRODUCT_NAME_NAME', caseObj.Asset.Name);
            emailBody = emailBody.replace('CASE_CASENUMBER', caseObj.CaseNumber);
            emailBody = emailBody.replace('CASE_TTI_BRAND_SERVICE_PHONE', caseObj.TTI_Brand_Service_Phone__c);
            emailBody = emailBody.replace('Brand_LogoURL', caseObj.TTI_Brand_Logo_URL__c);
            List<Messaging.SingleEmailMessage> lstemailToSent = new List<Messaging.SingleEmailMessage>();
            Messaging.EmailFileAttachment objAttachToSend = TTI_PdfAndEmailHandler.getAttachmentObjToSend(caseObj.CaseNumber+'_Quote.pdf',body);
            if(caseObj.TTI_Quote_to_Retailer__c){
                List<string> lstToAddress=new list<string>();
                lstToAddress.add(caseObj.Retailer_Email_Address__c);
                lstemailToSent.add(TTI_PdfAndEmailHandler.getEmailSendObj(emailBody,emailSubject,null,caseObj.Id,caseObj.TTI_CaseFromEmailId__c,objAttachToSend, new List<String>()));
                lstemailToSent[0].setToAddresses(lstToAddress);
            }
            else{
                lstemailToSent.add(TTI_PdfAndEmailHandler.getEmailSendObj(emailBody,emailSubject,caseObj.TTI_Customer_Contact__c,caseObj.Id,caseObj.TTI_CaseFromEmailId__c,objAttachToSend, new List<String>()));
            }
                
            if(lstemailToSent.size() > 0 && caseObj.TTI_Email_Notification_Opt_In__c){
                Messaging.SendEmailResult [] r1 = Messaging.sendEmail(lstemailToSent);  
            }
            TTI_PdfAndEmailHandler.insertAttachmentToCase(caseObj.CaseNumber+'_Quote.pdf',body,caseObj.id);
        }
        if(operation=='JobSummaryGeneration')
        {
            // INC0027645 - Change to Job summary sheet - 03/05/2023 - JAC
            // PageReference pdf = Page.TTI_JobSummary;
            PageReference pdf = new PageReference('');
            if(caseObj.Brand__c == 'Milwaukee'){
                pdf = Page.TTI_JobSummary_Milwaukee;
            } else{
                pdf = Page.TTI_JobSummary;
            }
            // INC0027645 - End
            string caseId=string.valueOf(caseObj.Id);
            pdf.getParameters().put('caseId',caseId);
            pdf.setRedirect(false);
            Blob body;
            if(!Test.isRunningTest()){
                body = pdf.getContent();
            }else{
                Test.setCurrentPage(pdf);
                TTI_JobSummaryReceiptController ObjTTI_JobSummaryReceiptController=new TTI_JobSummaryReceiptController();
                body=Blob.valueof('QWERTRERTYTRT');
            }
            TTI_PdfAndEmailHandler.insertAttachmentToCase(caseObj.CaseNumber+'_Job_Summary.pdf',body,caseObj.id);
            
        }
    }

